@model IEnumerable<ConstructionSpending.Models.Vacancy>

@{
    ViewData["Title"] = "CreateVacancy";
}

<h1>CreateVacancy</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.VacancyType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Value)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UoM)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SeasonallyAdjusted)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Market)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.VacancyType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Value)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UoM)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SeasonallyAdjusted)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Market.MarketID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.VacancyID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.VacancyID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.VacancyID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
